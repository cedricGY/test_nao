    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

    {% block stylesheets %}{% endblock %}

</head>
<body>
<div id="boxed-layout">

    <div id="topbar" class="header-top">
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <ul class="topbar-contact">
                        <li><i class="fa fa-phone-square"></i>03 27 99 99 99</li>
                    </ul>
                </div>
                <div class="col-md-6">
                    <ul class="social-icons pull-right">
                        <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                        <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                        <li><a href="#"><i class="fa fa-flickr"></i></a></li>
                        <li class="topbar-searchbox">
                            <form class="input-group" action="#" method="get">
                                <input type="text" class="form-control" name="q" placeholder="Rechercher">
                <span class="input-group-btn">
                <button class="btn btn-default"><i class="fa fa-search"></i></button>
                </span>
                            </form>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>


    <header id="header">
        <nav class="navbar yamm navbar-default navbar-static-top">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse"> <span class="sr-only">Toggle navigation</span> <span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> </button>
                    <button type="button" class="navbar-toggle-search" data-toggle="modal" data-target=".modal-recherche"> <span class="sr-only">Toggle navigation</span> <i class="fa fa-search"></i> </button>
                    <a class="navbar-brand" href="#"><img src="{{ asset('images/logo.png') }}" class="img-responsive" alt="" /></a></div>

                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav">
                        <li><a href="#">Accueil</a></li>
                        <li class="active dropdown"><a href="#" class="dropdown-toggle js-activated" data-toggle="dropdown" data-submenu>Observations <i class="fa fa-angle-down"></i></a>
                            <ul class="dropdown-menu">
                                <li><a href="#">Saisir observations</a></li>
                                <li><a href="#">Validations des saisies <i class="fa fa-lock"></i></a></li>
                            </ul>
                        </li>
                        <li class="dropdown"><a href="#" class="dropdown-toggle js-activated" data-toggle="dropdown">Recherche d'espèces <i class="fa fa-angle-down"></i></a>
                            <ul class="dropdown-menu">
                                <li><a href="#">Consulter la carte</a></li>
                                <li><a href="#">Consulter les fiches</a></li>
                                <li><a href="#" type="button" data-toggle="modal" data-target=".modal-telechargement">Exporter des données <i class="fa fa-lock"></i></a></li>
                            </ul>
                        </li>
                        <li><a href="#">Contact</a></li>
                        <li><a href="#">A propos</a></li>
                        <li><a href="#" type="button" data-toggle="modal" data-target=".modal-connexion">Connexion</a></li>
                    </ul>
                </div>

            </div>

        </nav>

    </header>

    <div class="clearfix"></div>

    {% block body %}{% endblock %}

    {#Modal exportation des données#}
    <div class="modal fade modal-telechargement" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div id="modal-row-connexion">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    </div>
                    <div class="modal-body text-center">
                        <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Ducimus eligendi explicabo laborum libero minima modi!</p>
                        <button type="button" class="btn btn btn-theme center-block pr-50 pl-50">Télécharger</button>
                        <p><em>(Format csv)</em></p>
                    </div>

                </div>
            </div>
        </div>
    </div>

    {#Modal recherche#}
    <div class="modal fade modal-recherche" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div id="modal-row-connexion">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    </div>
                    <div class="modal-body text-center">
                        <h3>Rechercher</h3>
                        <form class="input-group" action="#" method="get">
                            <input type="text" class="form-control" name="q" placeholder="Rechercher">
                <span class="input-group-btn">
                <button class="btn btn-default"><i class="fa fa-search"></i></button>
                </span>
                        </form>
                    </div>

                </div>
            </div>
        </div>
    </div>

    {#Modal connexion#}
    <div class="modal fade modal-connexion" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div id="modal-row-connexion">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title" id="myModalLabel">Connexion</h4>
                    </div>

                    <div class="modal-body">
                        <div class="loginform">
                            <fieldset>
                                <label>E-mail *</label>
                                <input class="form-control" type="email" name="email" required>
                                <label>Mot de passe *</label>
                                <input type="password" name="pwd" class="form-control" value="">
                            </fieldset>
                            <ul class="icon-arrow-list">
                                <li><div class="form-group"><input type="checkbox"> Se souvenir de moi ?</div></li>
                                <li><a href="#">Mot de passe oublié ?</a></li>
                                <li><button type="button" class="btn btn btn-theme pull-right">Connexion</button></li>
                            </ul>
                        </div>
                    </div>

                    <div class="modal-footer text-center">
                        <h4>Pas encore inscrit ?</h4>
                        <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Ducimus eligendi explicabo laborum libero minima modi!</p>
                        <button type="button" data-toggle="modal"  data-target=".modal-inscription" class="btn btn btn-theme">Inscription</button>
                    </div>

                </div>
            </div>
        </div>
    </div>

    {#Modal inscription#}
    <div class="modal fade modal-inscription" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div id="modal-row-inscription">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                        <h4 class="modal-title" id="myModalLabel">Inscription</h4>
                    </div>
                    <div class="modal-body">
                        <div class="mb-20">
                            <fieldset>
                                <div class="form-group">
                                    <label>Nom d\'utilisateur</label>
                                    <input class="form-control" type="text" name="username" placeholder="Ex: Paul86" required>
                                </div>
                                <div class="form-group">
                                    <label>E-mail</label>
                                    <input class="form-control" type="email" name="email" placeholder="Ex: canaris@oiseaux.fr" required>
                                </div>
                                <div class="form-group">
                                    <label>Mot de passe</label>
                                    <input type="password" name="pwd" class="form-control" value="">
                                </div>
                                <div class="form-group">
                                    <label>Repetez le mot de passe</label>
                                    <input type="password" name="repwd" class="form-control" value="">
                                </div>

                                <div class="form-group">
                                    <div class="radio">
                                        <label> <input type="radio" name="optionsRadios" id="optionsRadios1" value="option1" checked> Particulier </label>
                                    </div>
                                    <div class="radio">
                                        <label> <input type="radio" name="optionsRadios" id="optionsRadios2" value="option2"> Naturaliste </label>
                                    </div>
                                </div>

                                <div id="type-user">

                                </div>

                                <input class="submit btn btn-theme" name="submit" type="submit" value="Valider">
                            </fieldset>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <footer id="footer">
        <div class="container">
            <div class="row">
                <div class="col-md-3 col-md-offset-2">
                    <div class="footer-about"> <a href="#"><img src="{{ asset('images/logo.png') }}" alt="" /></a>
                        <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi.</p>
                        <div class="mb-20"></div>
                        <!-- end .mb-20 -->
                        <ul class="social-icons social-color">
                            <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                            <li><a href="#"><i class="fa fa-flickr"></i></a></li>
                            <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                            <li><a href="#"><i class="fa fa-skype"></i></a></li>
                            <li><a href="#"><i class="fa fa-dribbble"></i></a></li>
                            <li><a href="#"><i class="fa fa-instagram"></i></a></li>
                        </ul>
                        <div class="clearfix"></div>
                    </div>

                </div>

                <div class="col-md-3">
                    <h4>Lorem ipsum</h4>
                    <div class="titleline-footer"></div>

                    <ul class="footer-list">
                        <li><i class="fa fa-angle-right"></i><a href="#">Who we are</a></li>
                        <li><i class="fa fa-angle-right"></i><a href="#">About Our Company</a></li>
                        <li><i class="fa fa-angle-right"></i><a href="#">The Latest News</a></li>
                        <li><i class="fa fa-angle-right"></i><a href="#">Our Features</a></li>
                    </ul>
                </div>

                <div class="col-md-3">
                    <h4>Contact</h4>
                    <div class="titleline-footer"></div>

                    <ul class="footer-list">
                        <li>
                            <i class="fa fa-map-marker"></i><span>Adresse:</span><br>
                            8 rue des piafs
                        </li>
                        <li>
                            <i class="fa fa-phone"></i><span>Téléphone:</span><br>
                            0327999999
                        </li>
                        <li>
                            <i class="fa fa-envelope"></i><span>E-mail:</span><br>
                            <a href="mailto:#">mail@example.com</a>
                        </li>
                    </ul>
                </div>

            </div>

        </div>

    </footer>

    <div id="copyright">
        <div class="container">
            <div class="row">
                <div class="col-md-8">
                    <div class="copyright">&copy; 2016 Nos amis les oiseaux. All Rights Reserved.</div>
                    <!-- end .copyright -->
                </div>

                <div class="col-md-4">
                    <ul class="copyright-link">
                        <li><a href="#">Contact</a></li>
                    </ul>
                </div>
            </div>

        </div>

    </div>


    <a href="#" class="back-to-top">Back to Top</a>
</div>

